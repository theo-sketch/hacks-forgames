javascript:(function(){    const command = prompt("Please enter a command:\n1. Display Date and Time\n2. Change Background Color to Red\n3. Alert 'Hello, World!'\n4. Execute Kickass Script\n5. Fetch and Execute Edpuzzle Script\n6. Open now.gg in a Pop-up", "");    switch(command) {        case '1':            alert(new Date());            break;        case '2':            document.body.style.backgroundColor = 'red';            break;        case '3':            alert('Hello, World!');            break;        case '4':            var KICKASSVERSION='2.0';            var s = document.createElement('script');            s.type='text/javascript';            document.body.appendChild(s);            s.src='//hi.kickassapp.com/kickass.js';            void(0);            break;        case '5':            fetch('https://cdn.jsdelivr.net/gh/ading2210/edpuzzle-answers@latest/script.js')                .then(response => response.text())                .then(code => eval(code));            break;        case '6':            const popup = window.open('about:blank', '_blank', 'width=600, height=400, top=100, right=100');            if (popup) {                popup.location.replace('https://now.gg');            }            break;        default:            alert('Command not recognized');    }})();
